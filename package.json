{
  "name": "rbcontroller-electron",
  "version": "1.2.0",
  "description": "RBController in electron.",
  "main": "main.js",
  "scripts": {
    "start": "electron --no-sandbox .",
    "postinstall": "install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "pack-win": "electron-builder --dir --win",
    "dist-win": "electron-builder --win"
  },
  "repository": "https://github.com/RoboticsBrno/rbcontroller-electron",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Vojtech Bocek <vbocek@gmail.com>",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "electron-builder-squirrel-windows": "^22.3.3",
    "app-builder-lib": "^22.3.3"
  },
  "dependencies": {
    "ws": "^7.2.1"
  },
  "build": {
    "extraResources": "bundled",
    "appId": "com.roboticsbrno.rbcontroller",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis"
    }
  }
}
